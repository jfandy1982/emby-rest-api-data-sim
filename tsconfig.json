{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"outDir": "./dist",
		"removeComments": true,
		"strict": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noEmitOnError": true,
		"resolveJsonModule": true
	}
}
